# Django Settings
# Copy this file to .env and fill in your actual values
# SECURITY: Never commit .env file to version control!

# AI Act / Gemini API Configuration
# Get your API key from: https://aistudio.google.com/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# AI Act Store Configuration (optional)
# Store name is auto-generated when running setup_ai_act_store.py
# Leave empty if you want to use    the store from ai_act_store_info.txt
AI_ACT_STORE_NAME=

# AI Act Model Configuration
# Default: gemini-1.5-pro
# Options: gemini-1.5-pro, gemini-1.5-flash, gemini-2.0-flash-exp
AI_ACT_MODEL_NAME=gemini-1.5-pro

# Django Secret Key (for production)
# SECURITY WARNING: Change this in production!
# SECRET_KEY=your-secret-key-here

# Django Debug Mode (for production)
# Set to False in production
# DEBUG=True
